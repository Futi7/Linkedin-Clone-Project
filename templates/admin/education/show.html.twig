{% extends 'adminbase.html.twig' %}

{% block title %}Education{% endblock %}

{% block body %}
    <h1>Education</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ education.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ education.title }}</td>
            </tr>
            <tr>
                <th>Department</th>
                <td>{{ education.department }}</td>
            </tr>
            <tr>
                <th>Score</th>
                <td>{{ education.score }}</td>
            </tr>
            <tr>
                <th>Starting_date</th>
                <td>{{ education.startingDate }}</td>
            </tr>
            <tr>
                <th>Graduation_date</th>
                <td>{{ education.graduationDate }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('education_index') }}">back to list</a>

    <a href="{{ path('education_edit', {'id': education.id}) }}">edit</a>

    {{ include('admin/education/_delete_form.html.twig') }}
{% endblock %}

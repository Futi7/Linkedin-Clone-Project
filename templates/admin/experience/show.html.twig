{% extends 'adminbase.html.twig' %}

{% block title %}Experience{% endblock %}

{% block body %}
    <h1>Experience</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ experience.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ experience.title }}</td>
            </tr>
            <tr>
                <th>Company_worked</th>
                <td>{{ experience.companyWorked }}</td>
            </tr>
            <tr>
                <th>Details</th>
                <td>{{ experience.details }}</td>
            </tr>
            <tr>
                <th>Start_date</th>
                <td>{{ experience.startDate }}</td>
            </tr>
            <tr>
                <th>Date_of_quit</th>
                <td>{{ experience.dateOfQuit }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('experience_index') }}">back to list</a>

    <a href="{{ path('experience_edit', {'id': experience.id}) }}">edit</a>

    {{ include('admin/experience/_delete_form.html.twig') }}
{% endblock %}

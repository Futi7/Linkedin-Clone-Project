{% extends 'adminbase.html.twig' %}

{% block title %}General{% endblock %}

{% block body %}
    <h1>General</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ general.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ general.name }}</td>
            </tr>
            <tr>
                <th>Surname</th>
                <td>{{ general.surname }}</td>
            </tr>
            <tr>
                <th>Instagram</th>
                <td>{{ general.instagram }}</td>
            </tr>
            <tr>
                <th>Linkedin</th>
                <td>{{ general.linkedin }}</td>
            </tr>
            <tr>
                <th>Github</th>
                <td>{{ general.github }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ general.email }}</td>
            </tr>
            <tr>
                <th>Profile_image</th>
                <td>{{ general.profileImage }}</td>
            </tr>
            <tr>
                <th>About</th>
                <td>{{ general.about }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('general_index') }}">back to list</a>

    <a href="{{ path('general_edit', {'id': general.id}) }}">edit</a>

{% endblock %}
